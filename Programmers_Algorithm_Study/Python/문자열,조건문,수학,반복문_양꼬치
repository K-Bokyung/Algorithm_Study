# https://school.programmers.co.kr/learn/courses/30/lessons/120830

import math

def solution(n, k):
    answer = 0
    service_n = n // 10
    
    if service_n >= 1:
        answer = (n * 12000) + (k * 2000) - (service_n * 2000)
    else:
        answer = (n * 12000) + (k * 2000)
    return answer
